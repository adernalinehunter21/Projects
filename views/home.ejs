<%-include("partials/header")-%>
<div class="container">
  <h1>Home</h1>
  <p><%=homeContent %></p>
  <br />
  <%blogs.forEach(function(blog) {%>
  <h1><%=blog.title %></h1>
  <p>
    <%=blog.content.substring(0,100) + " ..." %>
    <a href="/posts/<%=blog.title%>">Read more</a>
  </p>

  <%});%>
</div>
<%-include("partials/footer")-%>
